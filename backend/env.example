# Backend Environment Variables
# Copy this file to .env and fill in your values

# Required: Google Gemini API key for LLM features
# Get this from https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Required: Browser Use Cloud API key for browser automation
# Get this from your Browser Use Cloud dashboard
BROWSER_USE_API_KEY=your_browser_use_api_key_here

# Optional: Enhanced logging level
LOG_LEVEL=INFO

# Optional: Convex URL (if backend needs to communicate with Convex)
# CONVEX_URL=your_convex_deployment_url_here
